{

    "type": "object",

    "required":["name", "timestamp", "nodes"],

    "properties": {
        "name": {
            "type": "string"
        },
        "timestamp": {
            "type": "date-time"
        },
        "root": {
            "$ref": "#/definitions/node"
        }
    },

    "definitions": {
        "node":{
            "type": "object",
                "required":["name", "id"],
                "properties": {
                "name": {
                    "type": "string"
                },
                "id": {
                    "type": "number"
                },
                "children": {
                    "type": "array",
                        "minItems": 1,
                        "items": {
                        "$ref": "#/definitions/node"
                    }
                },
                "attributes": {
                    "$ref": "#/definitions/attributeList"
                }
            }
        },
        "attributeList":{
            "type":"object",
                "patternProperties": {
                "^.*$":{
                    "type": "number"
                }
            }
        }
    }
}